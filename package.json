{
  "name": "react-rule-builder",
  "version": "0.1.0",
  "description": "A highly configurable, free-flow rule builder component developed with ReactJS",
  "keywords": [
    "rule",
    "builder",
    "rule builder",
    "react",
    "webpack",
    "typescript",
    "babel",
    "sass",
    "hmr"
  ],
  "author": "Henry Sue",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "./index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/henry0sue/react-rule-builder"
  },
  "bugs": {
    "url": "https://github.com/henry0sue/react-rule-builder/issues"
  },
  "homepage": "https://github.com/henry0sue/react-rule-builder",
  "scripts": {
    "build": "yarn run clean-dist && webpack --config=configs/webpack/build-lib.js",
    "build-demo": "yarn run clean-dist && webpack --config=configs/webpack/prod.js",
    "clean-dist": "rimraf dist/*; rimraf demo-dist/*",
    "lint": "eslint './src/**/*.{js,ts,tsx}' --quiet",
    "start": "yarn run start-dev",
    "start-dev": "webpack serve --config=configs/webpack/dev.js",
    "start-prod": "yarn run build-demo && node express.js",
    "test": "jest --coverage --watchAll --config=configs/jest.json"
  },
  "dependencies": {
    "lodash.filter": "^4.6.0",
    "lodash.find": "^4.6.0",
    "lodash.sortby": "^4.7.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-dropdown": "^1.9.1",
    "react-hot-loader": "^4.13.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.20",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.0.1",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "express": "^4.17.1",
    "file-loader": "^6.2.0",
    "foundation-sites": "^6.6.3",
    "html-webpack-plugin": "^5.0.0-alpha.10",
    "image-webpack-loader": "^7.0.1",
    "jest": "^26.6.3",
    "node-sass": "^5.0.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "webpack": "^5.14.0",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  }
